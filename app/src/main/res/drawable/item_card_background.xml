<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <solid android:color="@color/fab_backgroundcolor"/>
    <!-- 设置为弧形 弧形半径25dip-->
    <corners android:radius="25dip"/>
    <padding
        android:bottom="5dp"
        android:left="10dp"
        android:right="10dp"
        android:top="5dp"/>
    <!--设置描边 -->
    <stroke
        android:width="2dp"
        android:color="@color/fab_strokecolor"/>
</shape>
